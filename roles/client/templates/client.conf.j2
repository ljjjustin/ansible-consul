{
    "bind_addr": "{{ consul_bind_addr }}",
    "datacenter": "{{ consul_dc_name }}",
    "encrypt": "{{ consul_key }}",
    "data_dir": "/var/lib/consul",
    "log_level": "INFO",
    "enable_syslog": true,
    "enable_debug": true,
    "server": false,
    "rejoin_after_leave": true,
    "retry_join": [{{ consul_servers }}]
}
